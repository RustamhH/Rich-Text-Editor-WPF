<Application x:Class="Rich_Text_Editor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Rich_Text_Editor"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="Load" UriSource="assets\load button.png"></BitmapImage>
            <BitmapImage x:Key="DownArrow" UriSource="assets\downarrow.png"></BitmapImage>
            <BitmapImage x:Key="FillColor" UriSource="assets\fillcolor.png"></BitmapImage>
            <BitmapImage x:Key="Aligner" UriSource="assets\aligner.png"></BitmapImage>
        </ResourceDictionary>
    </Application.Resources>
</Application>
